{
    "name": "seguce92/alerts",
    "description": "Alert pNotify plugin, flash alerts integrated in Laravel.",
    "keywords": ["laravel", "bootstrap", "flash message", "pnotify"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Sergio Gualberto Cruz espinoza",
            "email": "seguce92@gmail.com",
            "homepage": "https://sergiocruzes.com/"
        }
    ],
    "require": {
        "php": ">=5.3",
        "prologue/alerts": "0.4.*"
    },
    "autoload": {
        "psr-4": {
            "Seguce92\\Alerts\\": "src/"
        }
    },
    "extra": {
